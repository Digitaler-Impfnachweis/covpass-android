	<string name="start_onboarding_title">Welcome to the CovPassCheck app</string>
	<string name="start_onboarding_message">Find out in just a few seconds if the person you are checking has a valid EU Covid-19 certificate.</string>
	<string name="start_onboarding_secure_title">Minimal data, maximum security</string>
	<string name="start_onboarding_secure_message">Data is read using just the QR code. No data is stored. You only see the information that is necessary.</string>
	<string name="start_onboarding_button_title">Get started</string>
	<string name="next_onboarding_page_button_title">Next</string>
