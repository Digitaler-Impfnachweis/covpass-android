	<string name="functional_validation_check_popup_unsuccessful_certificate_title">Certificate does not fulfil all conditions</string>
	<string name="functional_validation_check_popup_unsuccessful_certificate_subline">The checked person can see the reason in their CovPass App. The most common reasons:</string>
	<string name="functional_validation_check_popup_unsuccessful_certificate_subheadline_expiration">Test expired</string>
	<string name="functional_validation_check_popup_unsuccessful_certificate_subheadline_expiration_text">A negative rapid test has a maximal validity of 24 hours. A negative PCR test has a maximal validity of 72 hours.</string>
	<string name="functional_validation_check_popup_unsuccessful_certificate_subheadline_protection">Protection not yet valid</string>
	<string name="functional_validation_check_popup_unsuccessful_certificate_subheadline_protection_text">Individuals are not completely protected until 14 days after the second vaccination. A recovery from a Corona infection has to be at least 28 days in the past.</string>
	<string name="functional_validation_check_popup_unsuccessful_certificate_subheadline_uncompleted">Uncompleted vaccination series</string>
